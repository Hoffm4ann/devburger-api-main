{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ],
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb",
                "includeFiles": "src/**",
                "buildCommand": "npm run vercel-build"
            }
        }
    ]
}